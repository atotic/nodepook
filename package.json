    "cookie-parser": "^1.3.3",
    "debug": "^2.1.0",
    "express": "^4.9.0",
    "firebase": "^2.0.0",
    "firebase-token-generator": "^2.0.0",
    "flake-idgen": "0.1.4",
    "formidable": "^1.0.15",
    "gm": "^1.16.0",
    "int-encoder": "^0.1.4",
    "jade": "^1.6.0",
    "moment": "^2.8.3",
    "morgan": "^1.1.1",
    "serve-favicon": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "karma": "0.12.28",
    "karma-chrome-launcher": "*",
    "karma-firefox-launcher": "*",
