{
  "name": "pookio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./scripts/server.js",
    "devStart": "DEBUG=pook* nodemon ./scripts/server.js",
    "testServer": "find server -name \"*spec.js\" | xargs mocha",
